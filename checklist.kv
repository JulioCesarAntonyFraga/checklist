<Screen1@Screen>:
    name:"screen1"

    BoxLayout:
        orientation: "vertical"
        MDToolbar:
            title: "CheckLists"
            left_action_items: [["menu", lambda x: nav_drawer.toggle_nav_drawer()]]
            right_action_items: [["plus", lambda x: app.start_checklist()]]
            size_hint_y: 0.11
            elevation:5

        Widget:

    ScrollView:
        size_hint: (0.97, 0.89)
        pos_hint: {"center_x": .51, "y": 0.01}
        MDList:
            id: checklist
            


        

    MDNavigationDrawer:
        id: nav_drawer
        

        ContentNavigationDrawer:
            orientation: "vertical"
            padding: "8dp"
            spacing: "8dp"

            Image:
                id: avatar
                size_hint: (1,1)
                source: "mine.jpg"

            MDLabel:
                id: name_perfil_toolbar
                text: app.name_perfil_toolbar
                font_style: "Subtitle1"
                size_hint_y: None
                height: self.texture_size[1]

            MDLabel:
                id: email_perfil_toolbar
                text: app.email_perfil_toolbar
                size_hint_y: None
                font_style: "Caption"
                height: self.texture_size[1]


            DrawerList:
                id: md_list
                
                MDList:
                    OneLineIconListItem:
                        text: "CheckLists"
                        on_release: root.manager.current = "screen1"
                    
                        IconLeftWidget:
                            icon: "inbox"
                            on_release: root.manager.current = "screen1"
                    
                            
                    OneLineIconListItem:
                        text: "Minhas CheckList"
                        on_release: root.manager.current = "screen2"
                        
                    
                        IconLeftWidget:
                            icon: "inbox"
                            on_release: root.manager.current = "screen2"

                        
