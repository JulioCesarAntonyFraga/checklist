                        text: "Nova CheckList"
                        on_release: root.manager.current = "screen5"
                        
                    
                        IconLeftWidget:
                            icon: "inbox-arrow-up"
                            on_release: root.manager.current = "screen5"
                            
                    
                    OneLineIconListItem:
                        text: "Meu Perfil"
                        on_release: root.manager.current = "profile"
                    
                        IconLeftWidget:
                            icon: "account"
                            on_release: root.manager.current = "profile"

                    OneLineIconListItem:
                        text: "Sair"
                        on_release: app.show_alert_dialog()
                    
                        IconLeftWidget:
                            icon: "exit-to-app"
