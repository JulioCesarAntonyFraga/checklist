<CheckBoxLabel@ButtonBehavior+MDLabel>:
    text: ""


<LoginScreen>:

    name: "login"

    BoxLayout:
        orientation: "vertical"
        MDToolbar:
            title: "Auto LV"
            elevation:5
        Widget:


    MDTextField:
        halign: "center"
        id:email_input
        pos_hint: {"center_x":0.5,"center_y":0.70}
        size_hint: (0.7,0.1)
        hint_text : "Email"
        helper_text: "Obrigatório"
        helper_text_mode: "on_error"
        icon_right: "email"
        icon_right_color: app.theme_cls.primary_color
        required : True

    MDTextField:
        halign: "center"
        id: password_input
        pos_hint: {"center_x":0.5,"center_y":0.60}
        size_hint: (0.7,0.1)
        hint_text : "Senha"
        helper_text: "Obrigatório"
        helper_text_mode: "on_error"
        icon_right: "key"
        icon_right_color: app.theme_cls.primary_color
        required : True

    MDCheckbox:
        id: remember_input
        size_hint: None, None
        size: "48dp", "48dp"
        pos_hint: {"center_x": .16, "center_y": .49}
        active: False

    CheckBoxLabel:
        halign: "left"
        text: "Lembrar-me"
        pos_hint: {"center_x": .7, "center_y": .49}
        on_press: root.ids.remember_input.active = not root.ids.remember_input.active

    MDRaisedButton:
        halign: "center"
        text: "Entrar"
        pos_hint: {"center_x": .79, "center_y": .49}
        on_press: root.login()

    MDLabel:
        halign: "center"
        text:"ou"
        pos_hint: {"center_x": .5, "center_y": .39}

    MDRaisedButton:
        text: "Cadastre-se"
        pos_hint: {"center_x": .5, "center_y": .28}
        on_press: root.manager.current = "register"



<RegisterScreen>:
    name: "register"

    BoxLayout:
        orientation: "vertical"

        MDTextField:
            id: fisrt_name_input
            hint_text: "Nome"

        MDTextField:
            id: last_name_input
            hint_text: "Sobrenome"

        MDTextField:
            id: email_input
            hint_text: "Email"

        MDTextField:
            id: password1_input
            hint_text: "Senha"

        MDTextField:
            id: password2_input
            hint_text: "Repita a senha"

        MDRaisedButton:
            text: "Cadastrar"
            on_press: root.register()

        MDRaisedButton:
            text: "Entrar"
            on_press: root.manager.current = "login"


<ExpansionpanelContent@BoxLayout>:
    size_hint: 1, None
    height: self.minimum_height
    orientation: "vertical"

    adaptive_height: True

    ThreeLineIconListItem:
        id: panel_item_name

        text: "test"
        secondary_text: "Mobile"
        tertiary_text: "teste"

        IconLeftWidget:
            icon: "alert"


<Content@BoxLayout>:
    orientation: "vertical"
    padding: dp(10)
    spacing: dp(10)
    size_hint_y: None
    height: self.minimum_height

    OneLineAvatarIconListItem:
        IconLeftWidget:
            icon: "check"

        IconLeftWidget:
            icon: "file-pdf"

        IconLeftWidget:
            icon: "share-variant"

        IconRightWidget:
            icon: "delete"
            on_press:
                app.delete_item(self)


